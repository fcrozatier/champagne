	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch server",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "dev"],
			"autoAttachChildProcesses": true,
			"skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**"],
			"console": "integratedTerminal"
		}
	]
}
